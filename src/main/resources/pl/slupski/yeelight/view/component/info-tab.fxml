<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<ScrollPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="pl.slupski.yeelight.view.component.InfoTabController" fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0">
<content>
    <VBox prefHeight="200.0" prefWidth="100.0">
        <children>
            <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="200.0" VBox.vgrow="NEVER">
