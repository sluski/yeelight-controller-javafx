                <children>
                    <Label prefWidth="120.0" text="Color temperature">
                        <padding>
                            <Insets left="10.0" />
                        </padding>
                    </Label>
                    <Text fx:id="bulbColorTemperature" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
                        <HBox.margin>
                            <Insets left="20.0" />
                        </HBox.margin>
                    </Text>
                </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="200.0">
                <children>
                    <Label prefWidth="120.0" text="RGB">
                        <padding>
                            <Insets left="10.0" />
