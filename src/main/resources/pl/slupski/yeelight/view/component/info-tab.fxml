                    <Label prefWidth="120.0" text="ID">
                        <padding>
                            <Insets left="10.0" />
                        </padding>
                    </Label>
                    <Text fx:id="bulbId" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
                        <HBox.margin>
                            <Insets left="20.0" />
                        </HBox.margin>
                    </Text>
                </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="200.0">
                <children>
                    <Label prefWidth="120.0" text="Name">
                        <padding>
                            <Insets left="10.0" />
                        </padding>
                    </Label>
                    <Text fx:id="bulbName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
                        <HBox.margin>
                            <Insets left="20.0" />
